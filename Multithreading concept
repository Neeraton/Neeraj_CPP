class Multi extends Thread
{
	@Override
	public void run()
	{
		try
		{
			for(int i=0;i<5;i++)
				System.out.println("Neeraj");
				Thread.sleep(3000);
		}
			
			catch(InterruptedException i)
			{
				
			}
		
		}
	}
public class multithread {

	public static void main(String[] args) throws InterruptedException {
		
		Multi s=new Multi();
		s.start();
		
		for(int i=0;i<5;i++)
		{
			System.out.println("Chaurasia");
			Thread.sleep(3000);
		}

	}

}
